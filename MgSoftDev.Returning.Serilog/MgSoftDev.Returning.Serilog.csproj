        <AssemblyName>MgSoftDev.Returning.Serilog</AssemblyName>
        <RootNamespace>MgSoftDev.ReturningCore.Serilog</RootNamespace>
        <Authors>Miguel Fernando Garcias Salazar</Authors>
        <Company>MgSoftDev</Company>
        <Copyright>MgSoftDev</Copyright>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/MgSoftDev/MgSoftDev.Returning</PackageProjectUrl>
        <RepositoryUrl>https://github.com/MgSoftDev/MgSoftDev.Returning</RepositoryUrl>
        <PackageTags>Returning, Serilog</PackageTags>
        <Version>1.1.2</Version>
        <LangVersion>10</LangVersion>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <AssemblyVersion>1.1.2</AssemblyVersion>
