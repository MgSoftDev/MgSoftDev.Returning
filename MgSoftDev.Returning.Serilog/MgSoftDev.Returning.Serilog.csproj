    <ItemGroup>
      <ProjectReference Include="..\MgSoftDev.Returning\MgSoftDev.Returning.csproj" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="MgSoftDev.Returning" Version="1.0.7" />
